{
  "name": "acompi-back-end",
  "version": "1.0.0",
  "description": "",
  "main": "./src/app.ts",
  "scripts": {
    "dev": "tsc && node ./dist/src/app.js",
    "build": "tsc",
    "test": "dotenv -e .env.test jest -i && jest --maxWorkers=1 && npm run lint-and-fix",
    "start": "node ./dist/src/app.js",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "git-add": "git add . && npm run test",
    "git-push": "npm run lint-and-fix && git push",
    "testgenerate": "dotenv -e .env.test -- npx prisma generate",
    "migratetest": "dotenv -e .env.test -- npx prisma db push && dotenv -e .env.test -- npx prisma generate",
    "migratedev": "dotenv -e .env -- npx prisma db push && dotenv -e .env -- npx prisma generate",
    "pulltest": "dotenv -e .env.test -- npx prisma db pull && dotenv -e .env.test -- npx prisma generate",
    "pulldev": "dotenv -e .env -- npx prisma db pull && dotenv -e .env -- npx prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ifpi-picos/acompi-back-end.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ifpi-picos/acompi-back-end/issues"
  },
  "homepage": "https://github.com/ifpi-picos/acompi-back-end#readme",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/jsonwebtoken": "^8.5.9",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^3.1.0",
    "nodemailer": "^6.8.0",
    "supertest": "^6.3.0"
  },
  "devDependencies": {
    "@prisma/client": "^4.16.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.4",
    "@types/luxon": "^3.0.2",
    "@types/node": "^18.11.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.1.2",
    "prisma": "^4.16.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
